{
  "name": "retrofunding",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:admin": "turbo dev --filter=retrofunding-admin",
    "dev:voting": "turbo dev --filter=retrofunding-vote",
    "lint": "turbo lint",
    "pre-commit": "pnpm run --filter={retrofunding-admin,retrofunding-vote} pre-commit",
    "prepare": "husky && husky install",
    "e2e": "turbo e2e --filter=e2e-*",
    "wallet-cache": "turbo wallet-cache --filter=e2e-*"
  },
  "devDependencies": {
    "husky": "~8.0.3",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4",
    "moment": "2.30.1",
    "moment-timezone": "^0.5.46",
    "graphql-request": "^5.2.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
