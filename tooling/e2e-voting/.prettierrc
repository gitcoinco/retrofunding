{
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "all",
  "semi": true,
  "printWidth": 100,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)?$",
    "^@react(.*)?$",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^~(.*)$",
    "^(?!.*\\.css$)[./].*$",
    "\\.css$"
  ]
}
